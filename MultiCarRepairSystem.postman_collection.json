	"item": [
		{
			"name": "Authentication",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    pm.environment.set(\"jwt_token\", jsonData.token);",
									"    pm.environment.set(\"current_userId\", jsonData.userId);",
									"    pm.environment.set(\"current_emailId\", jsonData.emailId);",
									"    console.log(\"Token saved to environment\");",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"emailId\": \"user@example.com\",\n  \"password\": \"password123\"\n}"
						},
						"url": {
							"raw": "{{api_base}}/auth/login",
							"host": [
								"{{api_base}}"
							],
							"path": [
								"auth",
								"login"
							]
						},
						"description": "Login endpoint that authenticates user and returns JWT token. The token is automatically saved to the environment variable 'jwt_token' for use in subsequent requests."
					},
					"response": []
				}
			]
		},
